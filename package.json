{
  "name": "yalepaprika.com",
  "version": "1.0.0",
  "description": "yalepaprika.com",
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:js": "NODE_ENV=production rollup -c",
    "build:js:analyze": "NODE_ENV=production rollup -c --configVisualize",
    "build:css": "NODE_ENV=production postcss src/css/* --verbose --base src/css --dir public/assets/css",
    "watch": "npm-run-all --parallel watch:*",
    "watch:js": "rollup -c -w",
    "watch:css": "postcss src/css/**/*.css --base src/css --dir public/assets/css -w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ress": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@rollup/plugin-babel": "^5.0.3",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-typescript": "^4.1.2",
    "acorn-jsx": "^5.2.0",
    "cssnano": "^4.1.10",
    "fast-glob": "^3.2.2",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "rollup": "^2.14.0",
    "rollup-plugin-sizes": "^1.0.2",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-visualizer": "^4.0.4",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5"
  }
}
