{
  "name": "yalepaprika.com",
  "version": "1.0.0",
  "description": "yalepaprika.com",
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:js": "NODE_ENV=production rollup -c",
    "build:js:visualize": "NODE_ENV=production rollup -c --configVisualize",
    "build:css": "NODE_ENV=production postcss src/css/main.css --verbose --base src/css --dir public/assets/css",
    "watch": "npm-run-all --parallel watch:*",
    "watch:js": "rollup -c -w",
    "watch:css": "postcss src/css/main.css --base src/css --dir public/assets/css -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "eslint  --ignore-path .gitignore \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "lint": "eslint  --ignore-path .gitignore \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "@types/simplebar": "^5.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ress": "^2.0.4",
    "simplebar": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@rollup/plugin-babel": "^5.0.3",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-typescript": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "acorn-jsx": "^5.2.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "fast-glob": "^3.2.2",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "postcss-custom-media": "^7.0.8",
    "postcss-easy-import": "^3.0.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "rollup": "^2.15.0",
    "rollup-plugin-sizes": "^1.0.2",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-visualizer": "^4.0.4",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5"
  }
}
