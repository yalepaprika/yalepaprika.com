{
  "name": "yalepaprika.com",
  "version": "1.0.0",
  "description": "yalepaprika.com",
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:js": "NODE_ENV=production rollup -c",
    "build:js:visualize": "NODE_ENV=production rollup -c --configVisualize",
    "build:css": "NODE_ENV=production postcss src/css/main.css --verbose --base src/css --dir public/assets/css",
    "watch": "npm-run-all --parallel watch:*",
    "watch:js": "rollup -c -w",
    "watch:css": "postcss src/css/main.css --base src/css --dir public/assets/css -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "eslint  --ignore-path .gitignore \"src/**/*.{js,jsx}\" --fix",
    "lint": "eslint  --ignore-path .gitignore \"src/**/*.{js,jsx}\""
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@react-three/drei": "^7.17.1",
    "@react-three/fiber": "^7.0.19",
    "@swup/preload-plugin": "^1.0.3",
    "@swup/progress-plugin": "^1.0.1",
    "@swup/scroll-plugin": "^1.1.0",
    "@swup/slide-theme": "^1.0.5",
    "lazysizes": "^5.3.1",
    "plausible-tracker": "^0.3.1",
    "react": "^17.0.2",
    "@react-spring/core": "^9.3.0",
    "ress": "^3.0.0",
    "swup": "^2.0.14",
    "three": "^0.133.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.13",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.14.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^2.4.2",
    "acorn-jsx": "^5.3.1",
    "cssnano": "^4.1.10",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fast-glob": "^3.2.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.11",
    "postcss-cli": "^8.3.1",
    "postcss-custom-media": "^8.0.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-import": "^14.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "rollup": "^2.43.1",
    "rollup-plugin-sizes": "^1.0.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^4.2.2"
  }
}
